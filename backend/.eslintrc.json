{
  "env": {
    "node": true
  },
  "extends": ["airbnb-base", "plugin:security/recommended", "plugin:prettier/recommended"],
  "plugins": ["security", "prettier"],
  "parserOptions": {
    "ecmaVersion": 2018
  },
  "rules": {
    "no-console": "error",
    "func-names": "off",
    "new-cap": "off",
    "no-underscore-dangle": "off",
    "no-unused-vars": "off",
    "consistent-return": "off",
    "security/detect-object-injection": "off"
  }
}
