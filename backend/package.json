{
  "name": "swj-backend",
  "version": "1.0.0",
  "description": "Backend for the SWJ Project",
  "main": "app.ts",
  "scripts": {
    "clean": "rimraf dist/*",
    "lint": "tslint -c tslint.json -p tsconfig.json --fix",
    "tsc": "tsc",
    "build": "npm-run-all --silent clean lint tsc",
    "dev:start": "npm-run-all --silent build start",
    "dev": "nodemon --watch src -e ts --exec NODE_ENV=DEV npm run dev:start",
    "start": "npm run build && node dist/app.js"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "bcrypt": "^5.0.1",
    "compression": "^1.7.4",
    "cookie-session": "^2.0.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.0",
    "express": "^4.17.2",
    "express-mongo-sanitize": "^2.2.0",
    "helmet": "^5.0.2",
    "mongoose": "^6.2.1",
    "passport": "^0.5.2",
    "passport-google-oauth20": "^2.0.0",
    "validator": "^13.7.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/compression": "^1.7.2",
    "@types/cookie-session": "^2.0.44",
    "@types/cors": "^2.8.12",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.13",
    "@types/node": "^17.0.17",
    "@types/passport": "^1.0.7",
    "@types/passport-google-oauth": "^1.0.42",
    "@types/passport-google-oauth20": "^2.0.11",
    "@types/validator": "^13.7.1",
    "concurrently": "^7.0.0",
    "cross-env": "^7.0.3",
    "nodemon": "^2.0.15",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "tslint": "^6.1.3",
    "typescript": "^4.5.5"
  }
}
